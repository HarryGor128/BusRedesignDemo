{
  "name": "BusRedesignDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --port 9000",
    "ios": "react-native run-ios --port 9000",
    "lint": "eslint .",
    "start": "react-native start --experimental-debugger --port 9000",
    "test": "jest",
    "buildAndroidWin": "cd android && gradlew assembleRelease && cd .. && powershell -Command \"Get-ChildItem .\\android\\app\\build\\outputs\\apk\\release\\app-release.apk | Rename-Item -NewName { 'BusRedesignDemo_'+(Get-Date -Format 'yyyyMMdd_HHmmss')+'.apk' }\"",
    "buildAndroidMac": "cd android && ./gradlew assembleRelease && cd .. && mv ./android/app/build/outputs/apk/release/app-release.apk ./android/app/build/outputs/apk/release/BusRedesignDemo_$(date +'%Y%m%d_%H%M%S').apk",
    "cleanAndroidWin": "cd android && gradlew clean && cd ..",
    "cleanAndroidMac": "cd android && ./gradlew clean && cd ..",
    "portRestart": "net stop winnat && net start winnat",
    "reinstall": "rm -rf node_modules && rm package-lock.json && npm install",
    "reset": "npm start -- --reset-cache",
    "adbReverse": "adb reverse tcp:8081 tcp:8081",
    "adbRestart": "adb kill-server && adb start-server",
    "stopGradleWin": "cd android && gradlew --stop && cd ..",
    "stopGradleMac": "cd android && ./gradlew --stop && cd ..",
    "wifiConnect": "adb connect 192.168.10.54:43405",
    "wifiDisconnect": "adb disconnect 192.168.10.54:43405",
    "pod": "npx pod-install --repo-update",
    "devtool": "npx react-devtools"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.74.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.86",
    "@react-native/eslint-config": "0.74.86",
    "@react-native/metro-config": "0.74.86",
    "@react-native/typescript-config": "0.74.86",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
